{
    "defaultSeverity": "error",
    "extends": "typestrict",
    "linterOptions": {
      "exclude": [
        "**/*.spec.js",
        "**/node_modules/**/*"
      ]
    },
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single"],
      "semicolon": [true, "never"],
      "trailing-comma": [
        true,
        {
          "multiline": "always",
          "singleline": "never"
        }
      ]
    },
    "rulesDirectory": []
}
